<h1>Training Plan</h1>
<%= @user.name %>
<p>
  <%= link_to "Add planned session", new_user_training_plan_path %>
</p>  
<% @training_plans.each do |x| %>
  <ol>
    <li>
      <%= x.plan_date %>
      <%= x.plan_workout %>
      <%= "Q" if x.plan_q   %>
      <%= x.plan_miles %>
      <%= link_to "Edit", edit_user_training_plan_path(current_user, x.id) %> 
      <%= link_to "Delete", user_training_plan_path(current_user, x.id), method: :delete, data: { confirm: "Are you sure?" } %> 
    </li>
  </ol>
<% end %>